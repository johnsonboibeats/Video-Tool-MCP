{
  "$schema": "https://railway.app/schemas/railway.schema.json",
  "name": "image-tool-mcp",
  "variables": {
    "OPENAI_API_KEY": {
      "value": "",
      "isSensitive": true
    },
    "GOOGLE_SERVICE_ACCOUNT_JSON": {
      "value": "",
      "isSensitive": true,
      "isOptional": true
    },
    "PORT": {
      "value": "8000",
      "isOptional": true
    }
  },
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python server.py",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300
  }
}